{
  "name": "helia-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "build:alpha": "ng build --configuration=alpha",
    "backup-db:dev": "chmod a+x ../bash/backup-db-dev.sh && bash ../bash/backup-db-dev.sh",
    "restore-db:dev": "chmod a+x ../bash/restore-db-dev.sh && bash ../bash/restore-db-dev.sh",
    "backup-db:alpha": "chmod a+x ../bash/backup-db-alpha.sh && bash ../bash/backup-db-alpha.sh",
    "restore-db:alpha": "chmod a+x ../bash/restore-db-alpha.sh && bash ../bash/restore-db-alpha.sh",
    "flyway-info:dev": "chmod a+x ../bash/flyway-info-db-dev.sh && bash ../bash/flyway-info-db-dev.sh",
    "flyway-migrate:dev": "chmod a+x ../bash/flyway-migrate-db-dev.sh && bash ../bash/flyway-migrate-db-dev.sh",
    "ncu": "ncu -u",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pretty": "prettier --write \"./**/*.{ts,js,jsx,json}\"",
    "ngupdate": "ng update @angular/core @angular/cli",
    "build-docker:alpha": "chmod a+x ../bash/build-docker-alpha.sh && bash ../bash/build-docker-alpha.sh",
    "deploy-docker:alpha": "chmod a+x ../bash/deploy-docker-alpha.sh && bash ../bash/deploy-docker-alpha.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.10",
    "@angular/common": "^13.3.10",
    "@angular/compiler": "^13.3.10",
    "@angular/core": "^13.3.10",
    "@angular/forms": "^13.3.10",
    "@angular/localize": "^13.3.10",
    "@angular/platform-browser": "^13.3.10",
    "@angular/platform-browser-dynamic": "^13.3.10",
    "@angular/router": "^13.3.10",
    "@types/offscreencanvas": "^2019.7.0",
    "bootstrap": "^5.1.3",
    "clipboard": "^2.0.11",
    "gl-vec2": "^1.3.0",
    "jquery": "^3.6.0",
    "pako": "^2.0.4",
    "prettier": "^2.6.2",
    "regl": "^2.1.0",
    "rng": "^0.2.2",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-devkit/build-angular": "^13.3.7",
    "@angular/cli": "^13.3.7",
    "@angular/compiler-cli": "^13.3.10",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/jquery": "^3.5.14",
    "@types/node": "^17.0.36",
    "jasmine-core": "~4.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "javascript-obfuscator": "^4.0.0",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "^2.0.0",
    "npm-check-updates": "^13.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~10.8.0",
    "tslint": "~6.1.3",
    "typescript": "~4.7.2",
    "webpack-obfuscator": "^3.5.1",
    "yarn": "^1.22.18"
  }
}
