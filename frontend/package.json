{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "backup-db": "export PGPASSWORD='thisiswherewefight' && pg_dump -h 127.0.0.1 -d helia -U developer -f ../schema.sql -s && pg_dump -h 127.0.0.1 -d helia -U developer -f ../testdb.sql",
    "restore-db": "export PGPASSWORD='thisiswherewefight' && echo 'drop owned by developer;' | psql -h 127.0.0.1 -d helia -U developer && psql -h 127.0.0.1 -d helia -U developer < ../testdb.sql",
    "ncu": "ncu -u",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pretty": "prettier --write \"./**/*.{ts,js,jsx,json}\"",
    "ngupdate": "ng update @angular/core @angular/cli"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.2",
    "@angular/common": "^13.1.2",
    "@angular/compiler": "^13.1.2",
    "@angular/core": "^13.1.2",
    "@angular/forms": "^13.1.2",
    "@angular/localize": "^13.1.2",
    "@angular/platform-browser": "^13.1.2",
    "@angular/platform-browser-dynamic": "^13.1.2",
    "@angular/router": "^13.1.2",
    "@types/offscreencanvas": "^2019.6.4",
    "bootstrap": "^5.1.3",
    "gl-vec2": "^1.3.0",
    "jquery": "^3.6.0",
    "pako": "^2.0.4",
    "prettier": "^2.5.1",
    "regl": "^2.1.0",
    "rng": "^0.2.2",
    "rxjs": "~7.5.2",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.3",
    "@angular/cli": "^13.1.3",
    "@angular/compiler-cli": "^13.1.2",
    "@types/jasmine": "~3.10.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^17.0.8",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.10",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "npm-check-updates": "^12.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.4.0",
    "tslint": "~6.1.3",
    "typescript": "~4.5.4",
    "yarn": "^1.22.17"
  }
}
